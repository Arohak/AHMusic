class {{ module_info.name }}ModuleConfigurator {

    func configureModuleForPresenter(presenter: AnyObject) {
        
        if let presenter = presenter as? {{ module_info.name }}Presenter {
            configure(presenter)
        }
    }

    private func configure(presenter: {{ module_info.name }}Presenter) {
        
        let viewController = {{ module_info.name }}ViewController()
        let interactor = {{ module_info.name }}Interactor()
        
        presenter.view = viewController
        presenter.interactor = interactor
        
        interactor.output = presenter
        viewController.output = presenter
    }
}
